{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "dependsOn": ["^clean"]
    },
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["tystie-base#build"]
    },
    "test": {
      "dependsOn": ["^build"]
    }
  }
}
